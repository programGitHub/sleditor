    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "cross-env": "^6.0.3",
    "prop-types": "^15.7.2"
